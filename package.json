{
  "name": "nestjs-api-structure",
  "version": "1.0.0",
  "description": "NestJS api structure",
  "main": "dist/app",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:all": "prettier --write \"**/*.{ts,js,json,html}\"",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@socket.io/redis-adapter": "^8.0.1",
    "@socket.io/redis-emitter": "^5.1.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.1",
    "bluebird": "^3.7.2",
    "bull": "^4.10.2",
    "cache-manager": "^5.1.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "current-git-branch": "^1.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "mysql2": "^3.0.1",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "^4.5.4",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.1.9",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.38",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.16",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/redis": "^4.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideongloomist/nestjs-api-structure"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/hideongloomist/nestjs-api-structure#readme",
  "author": "hideongloomist",
  "license": "MIT"
}
