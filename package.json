{
  "name": "nestjs-api-structure",
  "version": "1.0.0",
  "description": "NestJS api structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:all": "prettier --write \"**/*.{ts,md,js,json,html}\"",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/cli": "^9.1.3",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/platform-socket.io": "^9.1.2",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.2",
    "@socket.io/redis-adapter": "^7.2.0",
    "@socket.io/redis-emitter": "^5.0.0",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.0",
    "bluebird": "^3.7.2",
    "bull": "^4.9.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "helmet": "^6.0.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.2",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.37",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "^4.0.0",
    "@types/node": "^18.7.23",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/redis": "^4.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "jest": "^29.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideongloomist/nestjs-api-structure"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "homepage": "https://github.com/hideongloomist/nestjs-api-structure#readme",
