{
  "name": "nestjs-api-structure",
  "version": "1.0.0",
  "description": "NestJS api structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:all": "prettier --write \"**/*.{ts,md,js,json,html}\"",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.3",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.3",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/platform-socket.io": "^10.4.3",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.2",
    "bluebird": "^3.7.2",
    "bull": "^4.16.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "express": "^4.21.0",
    "express-session": "^1.18.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "helmet": "^7.1.0",
    "mysql2": "^3.11.3",
    "nanoid": "^3.3.7",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "ts-loader": "^9.5.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
